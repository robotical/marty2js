{
    "compilerOptions": {
      "esModuleInterop": true,
      "lib": [],
      "module": "ESNext", 
      "outDir": "./dist",
      "declaration": true,
      "rootDir": "./src",
      "strict": true,
      "target": "ES6",
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "downlevelIteration": true,
      "moduleResolution": "Node"
    },
    "exclude": ["node_modules","dist"],
    "include": ["src/**/*"]
  }
